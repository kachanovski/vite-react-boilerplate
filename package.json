    "@mantine/dates": "7.12.2",
    "@mantine/form": "7.12.2",
    "@mantine/hooks": "7.12.2",
    "@mantine/modals": "7.12.2",
    "@mantine/notifications": "7.12.2",
    "axios": "1.7.7",
    "dayjs": "1.11.13",
    "i18next": "23.15.1",
    "i18next-http-backend": "2.6.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "^15.0.1",
    "react-router-dom": "6.26.2",
    "sass": "1.78.0"
